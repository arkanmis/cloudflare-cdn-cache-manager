{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Start CDN Server",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/server.js",
      "envFile": "${workspaceFolder}/.env",
      "env": {
        "DISABLE_HTTPS_REDIRECT": "true"
      },
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Start CDN Server (Debug Mode)",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/server.js",
      "envFile": "${workspaceFolder}/.env",
      "env": {
        "DISABLE_HTTPS_REDIRECT": "true"
      },
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "outputCapture": "std"
    }
  ]
}
